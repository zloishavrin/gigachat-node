{
  "name": "gigachat-node",
  "version": "2.4.0",
  "description": "The unoffical JavaScript/TypesSript library for the GigaChat API",
  "main": "index.js",
  "type": "commonjs",
  "repository": "git+https://github.com/zloishavrin/gigachat-node.git",
  "homepage": "https://zloishavrin.github.io/gigachat-node/",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "example": "nodemon",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "minify": "for file in dist/**/*.js; do terser $file -c -m -o $file; done"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "ai",
    "gigachat",
    "sber",
    "kandynski",
    "Сбер",
    "Гигачат",
    "Кандинский",
    "llm"
  ],
  "author": "Nikita Shavrin",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.9.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.5.1",
    "terser": "^5.39.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.3"
  }
}
