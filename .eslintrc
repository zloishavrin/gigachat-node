{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn",
      "quotes": ["error", "double"],
      "indent": ["error", 4, { "SwitchCase": 1 }],
      "object-curly-spacing":["warn", "always"]
    }
  }
  